{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Generate Bindings",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build binding generator",
			"program": "${workspaceFolder}/BindingGenerator/bin/Debug/net7.0/BindingGenerator.dll",
			"args": [],
			"cwd": "${workspaceFolder}",
			"stopAtEntry": false,
			"console": "internalConsole",
			"logging": {
				"moduleLoad": false,
			}
		},
		{
			"name": "Godot",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/godot/godot.windows.editor.x86_64.exe",
			"args": [
				"-e",
				"${workspaceFolder}/Example/project.godot",
				//"--quit",
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/Example",
			"environment": [],
			"console": "integratedTerminal",
			"preLaunchTask": "publish",
			"requireExactSource": true,
		},
	]
}
